# BitAds V3 Validator Environment Variables
# Copy this file to .env and fill in your values

# Bittensor Configuration
# NETUID: 16 for finney (mainnet), 368 for test (testnet)
NETUID=16
SUBTENSOR_NETWORK=finney

# Wallet Configuration
WALLET_NAME=default
WALLET_HOTKEY=default

# Logging
LOGGING_LEVEL=info

# Prometheus Telemetry
# Metrics server port (Prometheus will scrape /metrics on this port).
# Defaults to 9100 if not set.
# METRICS_PORT=9100

